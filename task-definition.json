{
    "family": "mlops-deply-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["EC2"],
    "cpu": "1024",
    "memory": "3072",
    "executionRoleArn": "arn:aws:iam::804923754854:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "mlops-deploy-app",
            "image": "804923754854.dkr.ecr.us-east-2.amazonaws.com/mlops-deploy-app",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/mlops-deply-task",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs",
                    "awslogs-create-group": "true"
                }
            }
        }
    ]
}