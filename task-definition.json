{
  "family": "mlops-deploy-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::804923754854:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "mlops-deploy-app",
      "image": "804923754854.dkr.ecr.us-east-2.amazonaws.com/mlops-deploy-app",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mlops-deploy-task",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
